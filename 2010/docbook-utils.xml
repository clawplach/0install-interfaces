<interface uri="http://faui2o2b.informatik.uni-erlangen.de/interfaces/2010/docbook-utils.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>docbook-utils</name>
  <summary>Shell scripts for managing DocBook documents</summary>
  <description>
This package contains scripts are for easy conversion from DocBook
files to other formats (for example, HTML, RTF, and PostScript), and
for comparing SGML files.
</description>
  <homepage>http://sources.redhat.com/docbook-tools/</homepage>
  <category>Utility</category>
  <group/>
  <package-implementation distribution="Debian RPM" package="docbook-utils"/>
  <package-implementation distributions="Gentoo" package="app-text/docbook-sgml-utils"/>
</interface>
