<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='interface.xsl'?>
<interface uri="http://hircus.multics.org/interfaces/2011/dbus-glib.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>dbus-glib</name>
  <summary>GLib bindings for D-Bus</summary>
  <description>
D-Bus add-on library to integrate the standard D-Bus library with the GLib
thread abstraction and main loop.</description>
  <homepage>http://www.freedesktop.org/software/dbus/</homepage>
  <category>System</category>
  <group/>
  <package-implementation distributions="RPM" package="dbus-glib"/>
  <package-implementation distributions="RPM" package="dbus-1-glib"/>
  <package-implementation distributions="Debian" package="libdbus-glib-1-2"/>
  <package-implementation distributions="Gentoo" package="dev-libs/dbus-glib"/>
</interface>
