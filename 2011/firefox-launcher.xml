<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='interface.xsl'?>
<interface uri="http://hircus.multics.org/interfaces/2011/firefox-launcher.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>Firefox (launcher)</name>
  <summary>Launcher script for Mozilla Firefox Web browser</summary>
  <description>
Mozilla Firefox is an open-source web browser, designed for standards
compliance, performance and portability.

This feed provides a launcher script that configures Firefox to pick up system
plugins, potentially wrapped using nspluginwrapper</description>
  <homepage>http://www.mozilla.com/firefox/</homepage>
  <icon href="http://people.mozilla.com/~faaborg/files/shiretoko/firefoxIcon/firefox-256.png" type="image/png"/>
  <category>Network</category>
  <group license="OSI Approved :: Mozilla Public License (MPL)">
    <group stability="testing">
      <implementation id="sha1new=0535f21c256afb60bb4e3eec63afc95298238e6f" main="firefox.sh" released="2011-08-30" version="0.1.1">
        <archive extract="firefox-launcher-0.1.1" href="http://hircus.multics.org/interfaces/dist/firefox-launcher-0.1.1.tar.bz2" size="606" type="application/x-bzip-compressed-tar"/>
      </implementation>
      <implementation id="sha1new=a7597e72f3788f14860678d563a4ceb2c0c9ba8c" main="firefox.sh" released="2011-08-17" version="0.1.0">
        <archive extract="firefox-launcher-0.1.0" href="http://hircus.multics.org/interfaces/dist/firefox-launcher-0.1.0.tar.bz2" size="528" type="application/x-bzip-compressed-tar"/>
      </implementation>
    </group>
    <requires interface="http://hircus.multics.org/interfaces/2011/firefox.xml">
      <environment insert="" mode="prepend" name="FIREFOX_HOME"/>
    </requires>
  </group>
</interface>
