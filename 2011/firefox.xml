<interface uri="http://mojo.informatik.uni-erlangen.de/interfaces/2011/firefox.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>Firefox</name>
  <summary>Mozilla Firefox Web browser</summary>
  <description>
Mozilla Firefox is an open-source web browser, designed for standards
compliance, performance and portability.</description>
  <homepage>http://www.mozilla.com/firefox/</homepage>
  <category>Network</category>
  <icon href="http://people.mozilla.com/~faaborg/files/shiretoko/firefoxIcon/firefox-256.png" type="image/png"/>
  <group license="OSI Approved :: Mozilla Public License (MPL)">
    <group released="2011-12-21" stability="stable" version="9.0.1">
      <implementation arch="Linux-x86_64" id="sha1new=289da143819cdfa0de11b0de92ec4ae56a4dc5c2" main="firefox">
        <archive extract="firefox" href="http://ftp.uni-kl.de/pub/mozilla/firefox/releases/9.0.1/linux-x86_64/en-US/firefox-9.0.1.tar.bz2" size="18728703" type="application/x-bzip-compressed-tar"/>
      </implementation>
      <implementation arch="Linux-i686" id="sha1new=d342912300cf0263f815fddd35731995f530919f" main="firefox">
        <archive extract="firefox" href="http://download.mozilla.org/?product=firefox-9.0.1&amp;os=linux&amp;lang=en-US" size="16840315" type="application/x-bzip-compressed-tar"/>
      </implementation>
    </group>
    <group released="2012-01-11" stability="testing" version="10.0" version-modifier="-pre4">
      <implementation arch="Linux-i686" id="sha1new=e9d17569e017225782ed8c5e203b3e3a8d302023" main="firefox">
        <archive extract="firefox" href="http://download.mozilla.org/?product=firefox-10.0b4&amp;os=linux&amp;lang=en-US" size="17751695" type="application/x-bzip-compressed-tar"/>
      </implementation>
      <implementation arch="Linux-x86_64" id="sha1new=ee1a49ea97570926707edbd9cc5f86f8b015485e" main="firefox">
        <archive extract="firefox" href="http://dm-download02.mozilla.org/pub/mozilla.org/firefox/releases/10.0b4/linux-x86_64/en-US/firefox-10.0b4.tar.bz2" size="19442658" type="application/x-bzip-compressed-tar"/>
      </implementation>
    </group>
    <requires interface="http://mojo.informatik.uni-erlangen.de/interfaces/2011/gtk2.xml"/>
  </group>
  <package-implementation distributions="Debian RPM" main="/usr/bin/firefox" package="firefox"/>
  <package-implementation distributions="RPM" main="/usr/bin/firefox" package="MozillaFirefox"/>
  <package-implementation distributions="Gentoo" main="/usr/bin/firefox" package="www-client/firefox"/>
  <package-implementation distributions="Debian" main="/usr/bin/iceweasel" package="iceweasel"/>
</interface>
