<interface uri="http://mojo.informatik.uni-erlangen.de/interfaces/2011/firefox.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>Firefox</name>
  <summary>Mozilla Firefox Web browser</summary>
  <description>
Mozilla Firefox is an open-source web browser, designed for standards
compliance, performance and portability.</description>
  <homepage>http://www.mozilla.com/firefox/</homepage>
  <category>Network</category>
  <icon href="http://people.mozilla.com/~faaborg/files/shiretoko/firefoxIcon/firefox-256.png" type="image/png"/>
  <group license="OSI Approved :: Mozilla Public License (MPL)">
    <group released="2011-11-01" stability="testing" version="8.0-pre6">
      <implementation arch="Linux-x86_64" id="sha1new=a25a25d21364ee2bf2b67b161fab3a5ef1df303c" main="firefox">
        <archive extract="firefox" href="http://dm-download02.mozilla.org/pub/mozilla.org/firefox/releases/8.0b6/linux-x86_64/en-US/firefox-8.0b6.tar.bz2" size="18604267" type="application/x-bzip-compressed-tar"/>
      </implementation>
      <implementation arch="Linux-i686" id="sha1new=8395d3ead0555be8eb8b93b52bbd3edb9906dec6" main="firefox">
        <archive extract="firefox" href="http://dm-download02.mozilla.org/pub/mozilla.org/firefox/releases/8.0b6/linux-i686/en-US/firefox-8.0b6.tar.bz2" size="16845394" type="application/x-bzip-compressed-tar"/>
      </implementation>
    </group>
    <group released="2011-09-29" stability="stable" version="7.0.1">
      <implementation arch="Linux-x86_64" id="sha1new=289602c79163f605a5e74cc5a718d1d7158522c2" main="firefox">
        <archive extract="firefox" href="http://3347-mozilla.voxcdn.com/pub/mozilla.org/firefox/releases/7.0.1/linux-x86_64/en-US/firefox-7.0.1.tar.bz2" size="16905662" type="application/x-bzip-compressed-tar"/>
      </implementation>
      <implementation arch="Linux-i686" id="sha1new=6afd7783f7f2e60a03c8ff06db9aab60ba0fdae2" main="firefox">
        <archive extract="firefox" href="http://download.mozilla.org/?product=firefox-7.0.1&amp;os=linux&amp;lang=en-US" size="15252909" type="application/x-bzip-compressed-tar"/>
      </implementation>
    </group>
    <requires interface="http://mojo.informatik.uni-erlangen.de/interfaces/2011/gtk2.xml"/>
  </group>
  <package-implementation distributions="Debian RPM" main="/usr/bin/firefox" package="firefox"/>
  <package-implementation distributions="RPM" main="/usr/bin/firefox" package="MozillaFirefox"/>
  <package-implementation distributions="Gentoo" main="/usr/bin/firefox" package="www-client/firefox"/>
  <package-implementation distributions="Debian" main="/usr/bin/iceweasel" package="iceweasel"/>
</interface>
