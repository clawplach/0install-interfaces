<interface uri="http://mojo.informatik.uni-erlangen.de/interfaces/2011/firefox.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>Firefox</name>
  <summary>Mozilla Firefox Web browser</summary>
  <description>
Mozilla Firefox is an open-source web browser, designed for standards
compliance, performance and portability.</description>
  <homepage>http://www.mozilla.com/firefox/</homepage>
  <category>Network</category>
  <icon href="http://people.mozilla.com/~faaborg/files/shiretoko/firefoxIcon/firefox-256.png" type="image/png"/>
  <group license="OSI Approved :: Mozilla Public License (MPL)">
    <group released="2011-08-25" stability="testing" version="7.0-pre2">
      <implementation arch="Linux-i686" id="sha1new=c9bdeffbe3abf08ef6f1f1f429f660531c607b30" main="firefox">
        <archive extract="firefox" href="http://releases.mozillaonline.com/pub/mozilla.org/firefox/releases/7.0b2/linux-i686/en-US/firefox-7.0b2.tar.bz2" size="15866147" type="application/x-bzip-compressed-tar"/>
      </implementation>
      <implementation arch="Linux-x86_64" id="sha1new=166700fbca9e26f354a52b93b2ff9e5bc549d2e6" main="firefox">
        <archive extract="firefox" href="http://releases.mozillaonline.com/pub/mozilla.org/firefox/releases/7.0b2/linux-x86_64/en-US/firefox-7.0b2.tar.bz2" size="17484590" type="application/x-bzip-compressed-tar"/>
      </implementation>
    </group>
    <group released="2011-08-16" stability="stable" version="6.0">
      <implementation arch="Linux-i686" id="sha1new=1c0a3fc42341f94ee0c4a0f0c2f1612abf1b1aaf" main="firefox">
        <archive extract="firefox" href="http://releases.mozilla.org/pub/mozilla.org/firefox/releases/6.0/linux-i686/en-US/firefox-6.0.tar.bz2" size="15083240" type="application/x-bzip-compressed-tar"/>
      </implementation>
      <implementation arch="Linux-x86_64" id="sha1new=f9738edb02faebb69e84e4e7d7fcf0d522a3ea9e" main="firefox">
    <archive extract="firefox" href="http://releases.mozilla.org/pub/mozilla.org/firefox/releases/6.0/linux-x86_64/en-US/firefox-6.0.tar.bz2" size="16667227" type="application/x-bzip-compressed-tar"/>
  </implementation>
    </group>
    <requires interface="http://mojo.informatik.uni-erlangen.de/interfaces/2011/gtk2.xml"/>
  </group>
  <package-implementation distributions="Debian RPM" main="/usr/bin/firefox" package="firefox"/>
