        <archive extract="firefox" href="http://pv-mirror02.mozilla.org/pub/mozilla.org/firefox/releases/10.0b3/linux-i686/en-US/firefox-10.0b3.tar.bz2" size="17750800" type="application/x-bzip-compressed-tar"/>
      </implementation>
      <implementation arch="Linux-x86_64" id="sha1new=a1fd339ad371b4664e1809d342bbef639f8b8c6e" main="firefox">
        <archive extract="firefox" href="http://pv-mirror02.mozilla.org/pub/mozilla.org/firefox/releases/10.0b3/linux-x86_64/en-US/firefox-10.0b3.tar.bz2" size="19423158" type="application/x-bzip-compressed-tar"/>
      </implementation>
    </group>
    <requires interface="http://mojo.informatik.uni-erlangen.de/interfaces/2011/gtk2.xml"/>
