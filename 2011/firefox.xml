<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='interface.xsl'?>
<interface uri="http://hircus.multics.org/interfaces/2011/firefox.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>Firefox</name>
  <summary>Mozilla Firefox Web browser</summary>
  <description>
Mozilla Firefox is an open-source web browser, designed for standards
compliance, performance and portability.</description>
  <homepage>http://www.mozilla.com/firefox/</homepage>
  <category>Network</category>
  <icon href="http://people.mozilla.com/~faaborg/files/shiretoko/firefoxIcon/firefox-256.png" type="image/png"/>
  <group license="OSI Approved :: Mozilla Public License (MPL)">
    <group released="2012-06-05" stability="stable" version="13.0">
      <implementation arch="Linux-x86_64" id="sha1new=3118f027ba4e0293f94895c4f3fd8ab827667d95" main="firefox">
        <archive extract="firefox" href="http://3347-mozilla.voxcdn.com/pub/mozilla.org/firefox/releases/13.0/linux-x86_64/en-US/firefox-13.0.tar.bz2" size="19807142" type="application/x-bzip-compressed-tar"/>
      </implementation>
      <implementation arch="Linux-i686" id="sha1new=43923b1f63212d0a0192e1ebb31332be2f578511" main="firefox">
        <archive extract="firefox" href="http://download.mozilla.org/?product=firefox-13.0&amp;os=linux&amp;lang=en-US" size="18076975" type="application/x-bzip-compressed-tar"/>
      </implementation>
    </group>
    <requires interface="http://hircus.multics.org/interfaces/2011/gtk2.xml"/>
  </group>
  <package-implementation distributions="Debian RPM" main="/usr/bin/firefox" package="firefox"/>
  <package-implementation distributions="RPM" main="/usr/bin/firefox" package="MozillaFirefox"/>
  <package-implementation distributions="Gentoo" main="/usr/bin/firefox" package="www-client/firefox"/>
  <package-implementation distributions="Debian" main="/usr/bin/iceweasel" package="iceweasel"/>
</interface>
