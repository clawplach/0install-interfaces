      <implementation arch="Linux-x86_64" id="sha1new=383252f740bd18a5eb2a01d031574aa48e990f13" main="thunderbird">
        <archive extract="thunderbird" href="http://pv-mirror01.mozilla.org/pub/mozilla.org/thunderbird/releases/10.0b5/linux-x86_64/en-US/thunderbird-10.0b5.tar.bz2" size="22066026" type="application/x-bzip-compressed-tar"/>
